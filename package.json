{
	"name": "@accuser/frouter",
	"version": "1.0.0",
	"description": "Functional TypeScript router for Cloudflare Workers",
	"type": "module",
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"import": "./dist/index.mjs",
				"types": "./dist/index.d.ts"
			}
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"dev": "wrangler dev --config wrangler.jsonc",
		"test": "vitest",
		"test:coverage": "vitest --coverage",
		"types": "wrangler types --config wrangler.jsonc"
	},
	"keywords": [],
	"author": {
		"email": "mhgibbons@me.com",
		"name": "Matthew Gibbons",
		"url": "https://github.com/accuser"
	},
	"license": "ISC",
	"packageManager": "pnpm@10.14.0",
	"devDependencies": {
		"@types/node": "^24.3.0",
		"@vitest/coverage-v8": "3.2.4",
		"@vitest/ui": "^3.2.4",
		"prettier": "^3.6.2",
		"tsup": "^8.5.0",
		"typescript": "^5.9.2",
		"vitest": "^3.2.4",
		"wrangler": "^4.31.0"
	}
}
